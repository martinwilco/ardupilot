include ../KakuteH7Mini/defaults.parm

# scripting enabled at startup
SCR_ENABLE 1

# battery settings
BATT_CAPACITY 7000
BATT_FS_CRT_ACT 1
BATT_FS_LOW_ACT 2
BATT_ARM_VOLT 15.4
BATT_CRT_VOLT 13.6
BATT_LOW_VOLT 14.4
MOT_BAT_VOLT_MAX 16.8
MOT_BAT_VOLT_MIN 13.2

# auto-enable BLHeli pass-thru support for all multicopter motors
SERVO_BLH_AUTO 1

# activate FPORT for RC
SERIAL6_OPTIONS 7